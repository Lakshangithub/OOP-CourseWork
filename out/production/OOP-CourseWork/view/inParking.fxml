<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="InParkingContext" prefHeight="450.0" prefWidth="570.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.inParkingController">
   <children>
      <ComboBox fx:id="cmbInParking" layoutX="49.0" layoutY="65.0" prefHeight="30.0" prefWidth="114.0" promptText="In Parking" style="-fx-background-color: #D5D5D5;" />
      <Button fx:id="btnAddVehicle" layoutX="248.0" layoutY="67.0" mnemonicParsing="false" onAction="#AddVehiOnAction" style="-fx-background-color: white; -fx-border-color: #0d30a4; -fx-border-width: 2;" text="Add Vehicle" />
      <Button fx:id="btnAddDriver" layoutX="359.0" layoutY="65.0" mnemonicParsing="false" onAction="#AddDriverOnAction" style="-fx-border-color: #0d30a4; -fx-background-color: white; -fx-border-width: 2;" text="Add Driver" />
      <Button fx:id="btnLogout" layoutX="468.0" layoutY="65.0" mnemonicParsing="false" onAction="#LogoutOnAction" style="-fx-background-color: white; -fx-border-width: 2; -fx-border-color: #0d30a4;" text="Log Out" />
      <TableView fx:id="tblInParking" layoutX="49.0" layoutY="142.0" prefHeight="248.0" prefWidth="482.0">
        <columns>
          <TableColumn fx:id="colVehiNo" prefWidth="120.0" text="Vehicle Number" />
          <TableColumn fx:id="colVehiType" prefWidth="120.0" text="Vehicle Type" />
            <TableColumn fx:id="colParkinhgSlot" prefWidth="120.0" text="Parking Slot" />
            <TableColumn fx:id="colParkingTime" prefWidth="120.0" text="Parking Time" />
        </columns>
      </TableView>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="30.0" prefWidth="570.0" style="-fx-background-color: #a9f470;" text="  Parking System" textFill="#141414">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
